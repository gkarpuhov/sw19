<?xml version="1.0" encoding="UTF-8" ?>
<DRPXML>
<A N='ID'>Null</A><!-- код печатной машины (Press)-->
<S N='Description'>
    <A N='Ru'>Настольный принтер Xerox</A>
    <A N='En'>Desktop Printer Xerox</A>
</S>
<S N='Media'>
    <A N='Roll' Type='Boolean'>false</A><!-- тип: рулон (true) или лист (false) -->
    <A N='IgnoreRoll' Type='Boolean'>true</A><!-- игнорировать тип рулон/лист : true или false -->
    <A N='Duplex' Type='Boolean'>true</A><!-- возможность двухсторонней печати -->
    <A N='MaxWidth' Type='Float'>9500</A><!-- максимальная ширина, мм -->
    <A N='MaxHeight' Type='Float'>99500</A><!-- максимальная высота для листа или длина для рулона, мм -->
    <A N='MinWidth' Type='Float'>1</A><!-- мин. ширина, мм -->
    <A N='MinHeight' Type='Float'>1</A><!-- мин. высота для листа или длина для рулона, мм -->
    <A N='MaxWeight' Type='Integer'>10000</A><!-- макс. вес, gsm -->
    <A N='MinWeight' Type='Integer'>0</A><!-- мин. вес, gsm -->
    <A N='MaxThickness' Type='Float'>10,500</A><!-- макс. толщина, мм -->
    <A N='MinThickness' Type='Float'>0,000</A><!-- мин. толщина, мм -->
</S>
<S N='PrintableArea'><!-- максимальная область печати, мм -->
    <A N='Width' Type='Float'>9500</A>
    <A N='Height' Type='Float'>9500</A>
</S>
<S N='UnprintableMargins'><!-- незапечатываемые поля, мм -->
    <A N='Left' Type='Float'>0</A>
    <A N='Top' Type='Float'>0</A>
    <A N='Right' Type='Float'>0</A>
    <A N='Bottom' Type='Float'>0</A>
</S>
<S N='Jdf'>
    <A N='Name'>Null</A>
    <A N='OutputFolder'>Null</A>
    <A N='OutputFilename'>%CID%-%ORDERNUMBER%-%ITEMID%-%JOBID%%VERSION%-t%PRODUCTKEYCODE%-%PRODUCTPARTCODE%</A>
</S>
<S N='Rip'>
    <A N='Name'>Null</A>
    <A N='OutputFolder'>Null</A>
</S>
<S N='Rip'>
    <A N='Name'></A>
    <A N='OutputFolder'>Null</A>
</S>
<S N='DefaultColorManagement' Enabled='false'>
	<A N='Convert' Action='' Key='' Type='Pitstop' Location='local'/>
	<RIP>
		<A N='CMYK'></A>
		<A N='RGB'></A>
		<A N='OUT'></A>
		<A N='DLP'></A>
	</RIP>
</S>
<S N='SubstrateList'>
    <C N='Params'>
        <A N='Filter' Type='Boolean'>true</A> <!-- true - использовать ТОЛЬКО позиции из ниже расположенного списка; false - использовать все позиции каталога, кроме включенных в спиcок -->
    </C>
    <C N='List'>
        <A>.+</A>
    </C>
</S>
</DRPXML>