<?xml version="1.0" encoding="UTF-8" ?>
<DRPXML>
<A N='ID'>Noritsu</A><!-- код печатной машины (Press)-->
<S N='Description'>
    <A N='Ru'>Фотохимия Noritsu</A>
    <A N='En'>Фотохимия Noritsu</A>
</S>
<S N='Media'>
    <A N='Roll' Type='Boolean'>true</A><!-- тип: рулон (true) или лист (false) -->
    <A N='IgnoreRoll' Type='Boolean'>false</A><!-- игнорировать тип рулон/лист : true или false -->
    <A N='Duplex' Type='Boolean'>false</A><!-- возможность двухсторонней печати -->
    <A N='MaxWidth' Type='Float'>900</A><!-- максимальная ширина, мм -->
    <A N='MaxHeight' Type='Float'>310</A><!-- максимальная высота для листа или длина для рулона, мм -->
    <A N='MinWidth' Type='Float'>210</A><!-- мин. ширина, мм -->
    <A N='MinHeight' Type='Float'>297</A><!-- мин. высота для листа или длина для рулона, мм -->
    <A N='MaxWeight' Type='Integer'>400</A><!-- макс. вес, gsm -->
    <A N='MinWeight' Type='Integer'>80</A><!-- мин. вес, gsm -->
    <A N='MaxThickness' Type='Float'>0,400</A><!-- макс. толщина, мм -->
    <A N='MinThickness' Type='Float'>0,100</A><!-- мин. толщина, мм -->
</S>
<S N='PrintableArea'><!-- максимальная область печати, мм -->
    <A N='Width' Type='Float'>900</A>
    <A N='Height' Type='Float'>310</A>
</S>
<S N='UnprintableMargins'><!-- незапечатываемые поля, мм -->
    <A N='Left' Type='Float'>0</A>
    <A N='Top' Type='Float'>0</A>
    <A N='Right' Type='Float'>0</A>
    <A N='Bottom' Type='Float'>0</A>
</S>
<S N='Jdf'>
    <A N='Name'>Noritsu</A>
    <A N='OutputFolder'>Noritsu</A>
    <A N='OutputFilename'>%ORDERNUMBER%-%ITEMID%-%JOBID%%VERSION%-t%CID%_%PRODUCTKEYCODE%-%FORMAT%-%BASE%-s%SUBSTRATE%-Q%PRNQTY%%IMPSYMBOL%</A>
</S>
<S N='Rip'>
    <A N='Name'>Noritsu</A>
    <A N='OutputFolder'>Noritsu</A>
</S>
<S N='DefaultColorManagement' Enabled='false'>
	<A N='Convert' Action='' Key='' Type='Pitstop' Location='local'/>
	<RIP>
		<A N='CMYK'></A>
		<A N='RGB'></A>
		<A N='OUT'></A>
		<A N='DLP'></A>
	</RIP>
</S>
<S N='SubstrateList'>
    <C N='Params'>
        <A N='Filter' Type='Boolean'>true</A> <!-- true - использовать ТОЛЬКО позиции из ниже расположенного списка; false - использовать все позиции каталога, кроме включенных в спиcок -->
    </C>
    <C N='List'>
        <A>.+</A>
    </C>
</S>
</DRPXML>